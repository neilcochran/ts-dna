{
  "name": "ts-dna",
  "version": "0.9.0",
  "license": "MIT",
  "description": "A simple typescript utility library for working with nucleic acids, amino acids, and polypeptides",
  "author": {
    "name": "Neil Cochran",
    "email": "dev.neil.cochran@pm.me",
    "url": "https://github.com/neilcochran"
  },
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "lint": "node scripts/lint.js",
    "test": "node scripts/test.js",
    "doc": "node scripts/documentation.js",
    "build": "node scripts/build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neilcochran/ts-dna.git"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "cspell": "^8.6.0",
    "eslint": "^8.57.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.0.4",
    "typedoc": "^0.25.12",
    "typescript": "^5.4.2"
  },
  "keywords": [
    "dna",
    "rna",
    "nucleic",
    "nucleic acid",
    "amino",
    "amino acid",
    "polypeptide",
    "genetics"
  ],
  "files": [
    "dist/**/*"
  ],
  "sideEffects": false
}
